
public static void main (String[]args){

    public void doStringStuff(String theString){
      Hashmap occurances = new Hashmap();
      for (int i=0; i<theString.length; i++){
        if (occurances.containsKey(theString.charAt(i))){
          occurances.put(theString.charAt(i), occurances.get(theString.charAt(i))+1);
        }
        else{
          occurances.put(theString.charAt(i), 1);
        }
      }
    }

    public void doStringStuff2(String theString){
